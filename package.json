{
  "name": "validate-dart-package-name",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.2",
  "description": "Check if a given string is a valid dart package name",
  "keywords": [
    "dart",
    "package",
    "name",
    "validate",
    "pub.dev"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/validate-dart-package-name#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/validate-dart-package-name/issues"
  },
  "repository": "ntnyq/validate-dart-package-name",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.1.0",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^20.16.13",
    "@vitest/coverage-v8": "^2.1.3",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.4",
    "pnpm": "^9.12.2",
    "prettier": "3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "vitest": "^2.1.3"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
