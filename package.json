{
  "name": "validate-dart-package-name",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.15.5",
  "description": "Check if a given string is a valid dart package name",
  "keywords": [
    "dart",
    "package",
    "name",
    "validate",
    "pub.dev"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/validate-dart-package-name#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/validate-dart-package-name/issues"
  },
  "repository": "ntnyq/validate-dart-package-name",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.21.0",
    "@ntnyq/prettier-config": "^1.21.0",
    "@types/node": "^20.11.28",
    "@vitest/coverage-v8": "^1.4.0",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.1.2",
    "pnpm": "^8.15.5",
    "prettier": "3.2.5",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.4.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
