{
  "name": "dart-packages",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r --stream build",
    "coverage": "vitest --coverage",
    "dev": "pnpm -r --stream dev",
    "lint": "eslint .",
    "prepare": "husky",
    "release": "run-s release:check release:publish",
    "release:check": "run-s build lint typecheck test",
    "release:publish": "pnpm run build && changeset publish",
    "test": "vitest",
    "typecheck": "vitest --typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@ntnyq/eslint-config": "^3.1.1",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.7.9",
    "@vitest/coverage-v8": "^2.1.3",
    "dart-reserved-words": "workspace:*",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.1",
    "pnpm": "^9.12.2",
    "prettier": "3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "validate-dart-package-name": "workspace:*",
    "vitest": "^2.1.3"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
